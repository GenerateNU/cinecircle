generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Bootcamp {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  name       String?
  fav_food   Json?    @db.Json

  @@map("bootcamp")
}

model Movie {
  movieId     String   @id @default(uuid()) @db.Uuid
  localRating Int
  imdbRating  Int
  languages   String[]
  title       String
  description String
  numRatings  Int

  @@map("movies")
}